using MYFINAL_ORDERS as MYFINAL_ORDERS;
using APISERVISE_ORDER_DETAILS as APISERVISE_ORDER_DETAILS;
view APISERVISE_ORDERS as select from MYFINAL_ORDERS as ORDERS mixin {
  ___RELPRODUCT : association[*] to APISERVISE_ORDER_DETAILS on ___RELPRODUCT.ORDERID_ORDERID = $projection.ORDERID;
} into {
  key ORDERS.ORDERID as ORDERID,
  ORDERS.AUXID,
  ORDERS.SHIPNAME,
  ___RELPRODUCT as RELPRODUCT
};
