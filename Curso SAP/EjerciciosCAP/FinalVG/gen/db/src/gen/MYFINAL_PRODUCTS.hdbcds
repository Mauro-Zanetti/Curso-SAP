using MYFINAL_ORDER_DETAILS as MYFINAL_ORDER_DETAILS;
entity MYFINAL_PRODUCTS {
  key PRODUCTID : Integer;
  PRODUCTNAME : String(5000);
  QUANTITYPERUNIT : String(5000);
  UNITPRICE : DecimalFloat;
  UNITSINSTOCK : Integer;
